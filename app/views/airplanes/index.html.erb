<p id="notice"><%= notice %></p>


<%= button_to 'Refresh', airplanesrefresh_path, method: :post %>


<h1>Viewable</h1>
  <table class="table">
    <thead>
      <tr>
        <th>Hex</th>
        <th>Sqawk</th>
        <th>Flight</th>
        <th>Lat</th>
        <th>Lon</th>
        <th>Validposition</th>
        <th>Altitude</th>
        <th>Vert rate</th>
        <th>Track</th>
        <th>Validtrack</th>
        <th>Speed</th>
        <th>Messages</th>
        <th>Seen</th>
        <th>Mlat</th>
        <th>range</th>
        <th colspan="3"></th>
      </tr>
    </thead>

<%if @planesinview != nil %>
    <tbody>
      <% @planesinview.each do |airplane| %>
        <tr>
          <td><%= airplane.hex %></td>
          <td><%= airplane.sqawk %></td>
          <td><%= airplane.flight %></td>
          <td><%= airplane.lat %></td>
          <td><%= airplane.lon %></td>
          <td><%= airplane.validposition %></td>
          <td><%= airplane.altitude %></td>
          <td><%= airplane.vert_rate %></td>
          <td><%= airplane.track %></td>
          <td><%= airplane.validtrack %></td>
          <td><%= airplane.speed %></td>
          <td><%= airplane.messages %></td>
          <td><%= airplane.seen %></td>
          <td><%= airplane.mlat %></td>
          <td><%= airplane.range %></td>
          <td><%= link_to 'Show', airplane %></td>
          <td><%= link_to 'Edit', edit_airplane_path(airplane) %></td>
          <td><%= link_to 'Destroy', airplane, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>

    </tbody>
<%end%>
  </table>



