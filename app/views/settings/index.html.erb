<p id="notice"><%= notice %></p>

<h1>Listing Settings</h1>

<table>
  <thead>
    <tr>
      <th>Use</th>
      <th>Location</th>
      <th>Origin lat</th>
      <th>Origin lon</th>
      <th>Cloudceiling</th>
      <th>Sunset</th>
      <th>Sunrise</th>
      <th>Dayrange</th>
      <th>Nightrange</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @settings.each do |setting| %>
      <tr>
        <td><%= setting.use %></td>
        <td><%= setting.originlat %></td>
        <td><%= setting.originlon %></td>

        <td><%= setting.cloudceiling %></td>
        <td><%= setting.sunset %></td>
        <td><%= setting.sunrise %></td>
        <td><%= setting.dayrange %></td>
        <td><%= setting.nightrange %></td>
        <td><%= link_to 'Show', setting %></td>
        <td><%= link_to 'Edit', edit_setting_path(setting) %></td>
        <td><%= link_to 'Destroy', setting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Setting', new_setting_path %>
